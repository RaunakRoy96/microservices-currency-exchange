package com.mindtree.currencyexchangeservice.service.impl;

import com.mindtree.currencyexchangeservice.exception.CurrencyExchangeException;

public class CurrencyExchangeNotFoundException extends CurrencyExchangeException {

	public CurrencyExchangeNotFoundException() {
		// TODO Auto-generated constructor stub
	}

	public CurrencyExchangeNotFoundException(String arg0) {
		super(arg0);
		// TODO Auto-generated constructor stub
	}

	public CurrencyExchangeNotFoundException(Throwable arg0) {
		super(arg0);
		// TODO Auto-generated constructor stub
	}

	public CurrencyExchangeNotFoundException(String arg0, Throwable arg1) {
		super(arg0, arg1);
		// TODO Auto-generated constructor stub
	}

	public CurrencyExchangeNotFoundException(String arg0, Throwable arg1, boolean arg2, boolean arg3) {
		super(arg0, arg1, arg2, arg3);
		// TODO Auto-generated constructor stub
	}

}
